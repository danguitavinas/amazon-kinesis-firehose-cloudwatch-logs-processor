{
  "name": "kinesis-firehose-inline-transform",
  "version": "0.1.0",
  "description": "An Amazon Kinesis Firehose stream processor that converts input records from VPC Flow Logs to raw ingestable format",
  "main": "node.js",
  "scripts": {
    "lint": "eslint -c ../.eslintrc --ignore-path ../.eslintignore .",
    "test": "node integration-test.js",
    "clean": "rm -f kinesis-firehose-inline-transform.zip",
    "pretest": "npm run lint",
    "build:zip": "zip -r kinesis-firehose-inline-transform.zip node.js lambda.json ",
    "build:template": "aws cloudformation package --template template.yaml --s3-bucket $npm_config_sam_s3_bucket_name --output-template-file template.output.yaml",
    "build:deployment":"aws cloudformation deploy --template-file $(pwd)/template.output.yaml  --stack-name $npm_config_parm_stack_name --capabilities \"CAPABILITY_IAM\"",
    "build": "npm run build:zip && npm run build:template && npm run build:deployment"
  },
  "keywords": [
    "splunk",
    "lambda",
    "kinesis",
    "firehose"
  ],
  "authors": [
    "Tarik Makota <tmak@amazon.com> (https://aws.amazon.com)",
    "Kevin Deng <chaodeng@amazon.com> (https://aws.amazon.com)"
  ],
  "license": "CC0-1.0",
  "dependencies": {
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1"
  }
}
